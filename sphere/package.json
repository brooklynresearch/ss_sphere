{
    "name": "com.ss.sphere",
    "version": "1.0.0",
    "displayName": "DisplaySphere",
    "cordova": {
        "platforms": [
            "android",
            "browser"
        ],
        "plugins": {
            "cordova-plugin-chrome-apps-sockets-udp": {},
            "cordova-plugin-crosswalk-webview": {
                "XWALK_VERSION": "23+",
                "XWALK_LITEVERSION": "xwalk_core_library_canary:17+",
                "XWALK_COMMANDLINE": "--disable-pull-to-refresh-effect",
                "XWALK_MODE": "embedded",
                "XWALK_MULTIPLEAPK": "true"
            },
            "cordova-plugin-file": {},
            "cordova-plugin-file-transfer": {},
            "cordova-plugin-insomnia": {},
            "cordova-plugin-statusbar": {},
            "cordova-plugin-whitelist": {},
            "cordova-hot-code-push-local-dev-addon": {},
            "org.jk.cordova.wakeupplugin": {}
        }
    },
    "dependencies": {
        "cordova-android": "^6.2.3",
        "cordova-browser": "~4.1.0",
        "cordova-hot-code-push-local-dev-addon": "~0.4.2",
        "cordova-plugin-chrome-apps-sockets-udp": "~1.3.0",
        "cordova-plugin-crosswalk-webview": "~2.3.0",
        "cordova-plugin-file": "~4.3.2",
        "cordova-plugin-file-transfer": "~1.6.2",
        "cordova-plugin-insomnia": "https://github.com/EddyVerbruggen/Insomnia-PhoneGap-Plugin.git",
        "cordova-plugin-statusbar": "~2.2.2",
        "cordova-plugin-wakeuptimer-jk": "git+https://github.com/jkhax0r/cordova-plugin-wakeuptimer.git",
        "cordova-plugin-whitelist": "1",
        "org.jk.cordova.wakeupplugin": "~1.0.2"
    }
}